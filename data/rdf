    <dct:title xml:lang="nl">Kandidatenlijsten Gemeenteraad 2022 (CSV)</dct:title>
    <dcat:accessURL rdf:resource="https://data.overheid.nl/sites/default/files/dataset/08b04bec-3332-4c76-bb0c-68bfaeb5df43/resources/GR2022_alle-kandidaten_2022-02-22T08.34.csv"/>
